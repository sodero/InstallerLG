(trap 1 (debug "a") (debug "b")) (debug "c") ; "","a b c 1",""
(trap 1 2 3 (debug @trap) (debug @trap)) (debug "c") ; "","3 3 c 1",""
(trap 1 2 "3" (debug @trap) (debug @trap)) (debug "c") ; "","3 3 c 1",""
(set a 3) (trap 1 "2" a (debug @trap) (debug @trap)) (debug "c") ; "","3 3 c 1",""
(set a 3) (trap 1 "2" b (debug @trap) (debug @trap)) (debug "c") ; "","Line 1: Undefined variable 'b' c 1",""
(set a 3) (trap 1 "2" a (debug b) (debug b)) (debug "c") ; "","<NIL> <NIL> c 1",""
(set @strict 0) (trap 1 "2" a (debug @trap) (debug @trap)) (debug "c") ; "","0 0 c 1",""
(trap (debug "fail") 1 2 3 (debug @trap) (debug @trap)) (debug "c") ; "","Line 1: syntax error, unexpected '(' '('",""
(trap 1 (debug "a") (debug (/ 0 0 )) (debug "b")) (debug "c") ; "","a 0 c 1",""
(trap 1 (debug "a") (debug (/ 0 0 )) (debug "b")) (debug "c") (debug (/ 0 0 )) ; "","Line 1: Division by zero '/' a 0 c 0 0",""
(trap 1 (message "!")) ; "","!00",""
(trap 1 ((message "!") (message "?"))) ; "","!0?00",""
(trap 1 (message "!") (message "?")) ; "","!0?00",""
(trap) ; "","Line 1: syntax error, unexpected ')' ')'",""
(trap err) ; "","Line 1: Undefined variable 'err' 0",""
(set @strict 0) (trap err) ; "","0",""
(set err 1) (trap err) ; "","0",""
(trap 1 err ((message "!") (message "?"))) ; "","Line 1: Undefined variable 'err' 0",""
(set @strict 0) (trap 1 err ((message "!") (message "?"))) ; "","!0?00",""
